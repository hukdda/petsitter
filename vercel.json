{
  "version": 2,
  "rewrites": [
    { "source": "/api/auth/social", "destination": "/api/auth.js" },
    { "source": "/api/admin/:path*", "destination": "/api/admin.js" },
    { "source": "/api/payment/:path*", "destination": "/api/payment.js" },
    { "source": "/api/services/:path*", "destination": "/api/services.js" },
    { "source": "/api/:path*", "destination": "/api/:path*.js" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ]
}